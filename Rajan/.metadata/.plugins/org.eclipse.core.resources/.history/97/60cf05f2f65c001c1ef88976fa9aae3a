package com.espire.palindrome;

import static org.junit.Assert.*;

import org.junit.Test;

/**
 * @author rajan.lahoti
 *
 */
public class PalindromeWithLibraryTest {

	@Test 
	public void testPalindromeUsingLibrary()
	{
		
	       assertFalse(PalindromeWithLibrary.isPalindrome("wonder"));
	       assertTrue(PalindromeWithLibrary.isPalindrome("nitin"));
	     
    }
}